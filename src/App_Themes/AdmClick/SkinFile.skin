<%--
Default skin template. The following skins are provided as examples only.

1. Named control skin. The SkinId should be uniquely defined because
   duplicate SkinId's per control type are not allowed in the same theme.

<asp:GridView runat="server" SkinId="gridviewSkin" BackColor="White" >
   <AlternatingRowStyle BackColor="Blue" />
</asp:GridView>

2. Default skin. The SkinId is not defined. Only one default 
   control skin per control type is allowed in the same theme.

<asp:Image runat="server" ImageUrl="~/images/image1.jpg" />
--%>

<asp:RegularExpressionValidator runat="server" style="font-size: 8pt; color: #FF3300"></asp:RegularExpressionValidator>
<asp:RequiredFieldValidator runat="server" style="font-size: 7pt; color: #FF3300"></asp:RequiredFieldValidator>
<asp:CompareValidator runat="server" style="font-size: 8pt; color: #FF3300"></asp:CompareValidator>


<asp:TextBox runat="server" style="font-family: Arial, Helvetica, sans-serif; font-size: 9pt; color: #333333; border-left: 1px solid #006699; border-right:1px solid #006699; border-top:1px solid #006699; border-bottom:1px solid #006699"></asp:TextBox>

<asp:Button runat="server" Text="Button" style="font-family: Arial, Helvetica, sans-serif; font-size: 9pt; color: #333333"/>
<asp:LinkButton runat="server" style="font-family: Arial, Helvetica, sans-serif; font-size: 9pt; color: #333333"></asp:LinkButton>
<asp:ImageButton runat="server" style="font-family: Arial, Helvetica, sans-serif; font-size: 9pt; color: #333333"/>
<asp:HyperLink  runat="server" style="font-family: Arial, Helvetica, sans-serif; font-size: 9pt; color: #333333"></asp:HyperLink>
    
<asp:DropDownList runat="server" style="font-family: Arial, Helvetica, sans-serif; color: #000000">
</asp:DropDownList>
    
<asp:ListBox runat="server" style="font-family: Arial, Helvetica, sans-serif; font-size: 9pt; color: #333333"></asp:ListBox>
<asp:CheckBox runat="server" />
    
<asp:Panel runat="server" style="width:740"></asp:Panel>   
<asp:RadioButton runat="server" style="font-family: Arial, Helvetica, sans-serif; font-size: 9pt; color: #333333"/>
<asp:RadioButtonList runat="server" style="font-family: Arial, Helvetica, sans-serif; font-size: 9pt; color: #333333">
</asp:RadioButtonList>
    
<asp:GridView runat="server" CellPadding="4"
    ForeColor="#333333" 
   
    Font-Size="10px">
    <FooterStyle BackColor="#507CD1" Font-Bold="True" ForeColor="White" />
        <SelectedRowStyle BackColor="#D1DDF1" Font-Bold="True" ForeColor="#333333" />
    <PagerStyle BackColor="#D1DDF1" ForeColor="Black" HorizontalAlign="Center" />
    <HeaderStyle BackColor="#507CD1" Font-Bold="True" ForeColor="Black" />
    <AlternatingRowStyle BackColor="White" />
</asp:GridView>


